{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint-fix": "npm run lint -- --fix",
    "build": "./node_modules/.bin/tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "NODE_ENV=test node ./src/test.js",
    "deploy": "firebase deploy --only functions",
    "publish-code": "gcloud app deploy",
    "logs": "firebase functions:log",
    "test-prod": "cd lib && NODE_RAW=true npm start",
    "test": "NODE_RAW=true NODE_ENV=test npx ts-node src/test.ts -P ./tsconfig.json"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/debug-agent": "^5.2.8",
    "@google-cloud/secret-manager": "^3.11.0",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "source-map-support": "^0.5.21",
    "typescript": "^3.8.0"
  },
  "private": true
}
